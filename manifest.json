{
  "manifest_version": 2,
  "name": "better-help-scout",
  "description": "Some UI tweaks to HelpScout",
  "version": "0.0.6",
  "icons": {
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://secure.helpscout.net/conversation/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
